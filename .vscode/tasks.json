{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "npm",
            "script": "type-check",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$tsc"],
            "label": "npm: type-check"
        },
        {
            "type": "npm",
            "script": "lint",
            "group": {
                "kind": "build"
                // "isDefault": true
            },
            "problemMatcher": ["$eslint-stylish"],
            "label": "npm: lint"
        }
    ]
}
