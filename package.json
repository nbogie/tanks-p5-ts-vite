{
    "name": "tanks-p5-ts-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev2": "yarn type-check && yarn lint && yarn format && yarn dev",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest --no-watch src",
        "type-check": "tsc --noEmit",
        "lint": "eslint --ext .ts src/",
        "type-check:watch": "tsc --noEmit --watch",
        "format": "prettier --write src/**/*.ts"
    },
    "devDependencies": {
        "@types/p5": "^1.6.2",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "eslint": "^8.46.0",
        "prettier": "^3.0.1",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vitest": "^0.34.1"
    },
    "dependencies": {
        "p5": "^1.7.0",
        "socket.io-client": "^4.7.2",
        "tone": "^14.7.77"
    }
}
